generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
}

model Meal {
  id            Int    @id @default(autoincrement())
  slug          String @unique
  title         String
  image         String
  summary       String
  instructions  String
  creator       String
  creator_email String

  @@map("meals")
}
